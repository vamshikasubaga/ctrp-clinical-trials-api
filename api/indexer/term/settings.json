{
  "number_of_shards": 1,
  "analysis": {
    "filter": {
      "autocomplete_filter": {
        "type": "edge_ngram",
        "min_gram": 1,
        "max_gram": 30,
        "token_chars": [
          "letter",
          "digit",
          "punctuation",
          "symbol"
        ]
      },
      "dot_delimiter": {
        "type" : "pattern_capture",
        "preserve_original" : 1,
        "patterns" : [
          "(\\p{Ll}+|\\p{Lt}+|\\p{Lu}+\\p{Ll}+|\\p{Lu}+)",
          "(\\d+)",
          "([\\p{Ll}+|\\p{Lt}+|\\p{Lu}+\\p{Ll}+|\\p{Lu}+]+\\d*|\\d+)",
          "([[\\p{Ll}+|\\p{Lt}+|\\p{Lu}+\\p{Ll}+|\\p{Lu}+]+\\d*|\\d+]+(\\p{Ll}+|\\p{Lt}+|\\p{Lu}+\\p{Ll}+|\\p{Lu}+))",
          "([\\d]+(\\p{Ll}|\\p{Lt}|\\p{Lu}\\p{Ll}|\\p{Lu})+\\d+)"
        ]
      }
    },
    "analyzer": {
      "autocomplete_index": {
        "type": "string",
        "tokenizer": "standard",
        "filter": [
          "standard","lowercase",
          "autocomplete_filter",
          "asciifolding",
          "dot_delimiter"
        ]
      },
      "autocomplete_search": {
        "type": "custom",
        "tokenizer": "whitespace",
        "filter": [
          "lowercase",
          "asciifolding"
        ]
      },
      "keyword_ci": {
        "tokenizer": "keyword",
        "filter": "lowercase"
      }
    }
  }
}
