{
  "name": "api",
  "version": "0.1.1",
  "authors": "Michael Balint <michael.balint@pif.gov> (https://presidentialinnovationfellows.gov/)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CBIIT/ctrp-clinical-trials-api.git"
  },
  "scripts": {
    "start": "node ./bin/www | ../common/node_modules/bunyan/bin/bunyan",
    "test": "node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha --compilers js:babel-core/register ./test/**/*.spec.js",
    "build-report": "node ./node_modules/nyc/bin/nyc report --reporter=text-lcov > coverage/coverage.lcov"
  },
  "nyc": {
    "exclude": [
      "public",
      "views",
      "test",
      "bin"
    ]
  },
  "dependencies": {
    "body-parser": "1.13.2",
    "bodybuilder": "git://github.com/bryanpizzillo/bodybuilder",
    "bunyan-middleware": "0.3.1",
    "cookie-parser": "1.3.5",
    "cors": "2.7.1",
    "debug": "2.2.0",
    "elasticsearch": "12.1.3",
    "express": "4.13.1",
    "git-rev": "^0.2.1",
    "jade": "1.11.0",
    "lodash": "4.17.4",
    "http-aws-es": "1.1.3",
    "marked": "^0.3.5",
    "moment": "2.13.0",
    "serve-favicon": "2.3.0",
    "axios": "0.16.2",
    "codacy-coverage": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "nyc": "*"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-plugin-lodash": "2.0.1",
    "babel-preset-es2015": "6.5.0",
    "chai": "3.2.0",
    "eslint": "1.10.2",
    "mocha": "2.4.5"
  }
}
