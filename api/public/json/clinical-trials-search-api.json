      "post": {
        "tags": [
          "cancer-clinical-trials"
        ],
        "summary": "Search Trials by POST",
        "description": "<h3><code>POST clinical-trials</code></h3><p>Same as the <code>GET</code> endpoint, but allows you to craft a JSON body as the request.</p><p>Example:</p><code>curl -XPOST 'https://clinicaltrialsapi.cancer.gov/v1/clinical-trials' \\<br>-H 'Content-Type: application/json' \\<br>-d '{<br>\"sites.org_state_or_province\": [\"CA\", \"OR\"],<br>\"record_verification_date_gte\": \"2016-06-01\",<br>\"include\": [\"nci_id\"]<br> }'</code>",
        "operationId": "searchTrialsByPost",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "status",
            "type": "string",
            "description": "Search Parameters",
            "required": true
          }
        ],
        "responses": {
          "500": {
            "description": "Exception occured at the backend (except EntityNotFoundException)."
          }
        }
      }
    },
      "/v1/term/{term_key}": {
        "get": {
          "tags": [
            "cancer-terms"
          ],
          "summary": "Get Term by 'term_key' value",
          "description": "<h3>GET term&lt;term_key&gt;</h3><p>Retrieves the term with supplied <code>term_key</code>.</p><p>Example:</p><p><a target='_blank' href=\"/v1/term/acute_myeloid_leukemia_in_remission\">Term with term_key value of acute_myeloid_leukemia_in_remission</a>.</p><hr>",
          "operationId": "getTermByTermKey",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "path",
              "name": "term_key",
              "description": "'term_key' value of term.",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Term details for the given term_key."
            },
            "400": {
              "description": "Bad Request."
            },
            "500": {
              "description": "An exception occurred at the backend."
            }
          }
        }
      },
